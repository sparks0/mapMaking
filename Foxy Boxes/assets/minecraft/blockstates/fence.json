{
    "multipart": [
		{	"when": {"north": "false","south":"false","east":"false","west":"false"},
			"apply": { "model": "oak_fence_post" }
		},
        {   "when": { "north": "true" },
            "apply": { "model": "striped_stone_outcrop", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "striped_stone_outcrop", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "striped_stone_outcrop", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "striped_stone_outcrop", "y": 270, "uvlock": true }
        }
    ]
}
